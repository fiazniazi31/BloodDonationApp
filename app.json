{
  "expo": {
    "name": "BloodDonation",
    "slug": "BloodDonation",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "light",

    "splash": {
      "image": "./assets/splash1.png",
      "resizeMode": "contain",
      "backgroundColor": "#DE3D3D"
    },
    "updates": {
